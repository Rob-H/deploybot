{
  "name": "deploy-bot",
  "version": "0.0.0",
  "description": "A slack bot to remind you when a commit gets to different environments",
  "main": "index.js",
  "scripts": {
    "pretest": "node ./node_modules/.bin/eslint .",
    "test": "node node_modules/.bin/istanbul cover node_modules/.bin/_mocha tests --recursive",
    "watch": "node node_modules/mocha/bin/mocha tests --recursive --watch"
  },
  "keywords": [
    "slack",
    "git"
  ],
  "author": "Rob Haley <robert.haley@scribestar.com>",
  "dependencies": {
    "body-parser": "^1.15.2",
    "botkit": "^0.2.2",
    "bunyan": "^1.8.5",
    "express": "^4.14.0",
    "nconf": "^0.8.4",
    "nedb": "^1.8.0",
    "nodegit": "^0.16.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "eslint": "^3.14.1",
    "istanbul": "^0.4.5",
    "mocha": "^3.1.2",
    "sinon": "^1.17.6"
  },
  "preferGlobal": true,
  "bin": {
    "deploybot": "index.js"
  }
}
